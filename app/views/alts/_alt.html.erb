
  <div id="<%= dom_id alt %>">
    <%= link_to alt, "aria-label": alt.body do %>
      <%= image_tag alt.image_url(:small), caption: alt.title, alt: alt.alt_texts.pluck(:body).last, height: alt.image(:small).height, width: alt.image(:small).width, class: "border rounded img-fluid w-100" %>
    <% end %>
  </div>
